#include <stdio.h>

int main() {
    int numero1, numero2;
    printf("Digite o primeiro numero: ");
    scanf("%i", &numero1);
    printf("Digite o segundo numero: ");
    scanf("%i", &numero2);
    int soma = numero1 + numero2;
    int subtracao = numero1 - numero2;
    int multiplicacao = numero1 * numero2;
    printf("Soma: %i", soma);
    printf("Subtração: %i", subtracao);
    printf("Multiplicação: %i", multiplicacao);

    return 0;
}
